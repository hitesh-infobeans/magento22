<?xml version="1.0"?>
<page
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer.wishlist.item.cart">
        	<action method="setTemplate">
                <argument name="template" xsi:type="string">Infobeans_CallForPrice::wishlist/item/column/cart.phtml</argument>
            </action> 
        </referenceBlock>
        <referenceBlock name="customer.wishlist.item.inner"> 
            <block class="Infobeans\CallForPrice\Block\Product\View\CallForPrice" after = "customer.wishlist.item.cart"  name="product.info.callforprice" as="callforprice" template="Infobeans_CallForPrice::wishlist/item/column/callforprice.phtml"/>
        </referenceBlock>
    </body>
</page>